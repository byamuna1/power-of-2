
// Sample code to perform I/O:

#include <iostream>

using namespace std;

int main() {
	 int t;
	 cin>>t;
	 while(t-->0)
	 {
	     int c;
	     char s[1000];
	     cin>>c>>s;
	     if(c==0)
	     {
	         for(int i=0;s[i]!='\0';i++)
	         {
	             if(s[i]>='A'&&s[i]<='Z')
	                cout<<"1";
	               else
	               cout<<"0";
	         }
	     }
	     else
	     {
	         for(int i=0;s[i]!='\0';i++)
	         {
	             if(s[i]>='A'&&s[i]<='Z')
	                cout<<"0";
	               else
	               cout<<"1";
	         }
	         
	     }
	     cout<<"\n";
	 }
}
