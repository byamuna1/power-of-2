
// Sample code to perform I/O:

#include <iostream>

using namespace std;

int main() {
     int t,sumx=0,sumy=0;
     cin>>t;
     while(t-->0)
     {
          int x,y;
          cin>>x>>y;
          sumx+=x;
          sumy+=y;
     }
     if(sumx>0&&sumy>0)
      cout<<"Q1";
     else if(sumx<0&&sumy<0)
      cout<<"Q3";
     else if(sumx<0&&sumy>0)
      cout<<"Q2";
      else if(sumx>0&&sumy<0)
       cout<<"Q4";
   }
     
